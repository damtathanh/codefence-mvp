{
    "ignore": {
        "useGitignore": true,
        "customPatterns": [
            "node_modules",
            "dist",
            "build",
            "yarn.lock",
            "package-lock.json",
            ".git",
            "public/assets",
            "**/*.svg",
            "**/*.png",
            "**/*.jpg",
            "**/*.ico"
        ]
    },
    "output": {
        "filePath": "repomix-output.xml",
        "style": "xml"
    }
}